{
  "expo": {
    "name": "İş Takip",
    "slug": "is-takip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "versionCode": 1,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.istakip.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.istakip.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulamanın fotoğraflarınıza erişmesine izin verin."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "16915656-126d-4963-8b75-f457880b1699"
      }
    }
  }
}
